{
  "name": "ai-tools-directory",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node generate-pages-local.js && node server.js",
    "build": "node -e \"import('./check-spaces.js').then(m => m.checkAllSpaces())\" && node generate-pages-netlify.js && vite build",
